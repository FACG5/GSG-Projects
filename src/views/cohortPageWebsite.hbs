<section class="silderSection">
  <section class="lazy slider ">
    {{#if res.studentResults.length}}
      {{#each res.studentResults}}
        <div class="sutdent">
          <a target="_blank" href="{{githublink}}">
            <img src="{{img_url}}">
          </a>
          <h4>{{name}}</h4>
        </div>
      {{/each}} 
    {{/if}}
  </section>
 </section>
</section>